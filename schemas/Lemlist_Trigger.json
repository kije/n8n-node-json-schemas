{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Lemlist Trigger",
  "description": "Handle Lemlist events via webhooks",
  "x-n8n-node-type": "lemlistTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "event": {
      "x-title": "Event",
      "default": "",
      "x-required": true,
      "type": "string",
      "enum": [
        "*",
        "contacted",
        "hooked",
