        "aircallInterested",
        "aircallNotInterested",
        "apiDone",
        "apiInterested",
        "apiNotInterested",
        "apiFailed",
        "linkedinVisitDone",
        "linkedinVisitFailed",
        "linkedinInviteDone",
        "linkedinInviteFailed",
        "linkedinInviteAccepted",
        "linkedinReplied",
        "linkedinSent",
        "linkedinVoiceNoteDone",
        "linkedinVoiceNoteFailed",
        "linkedinInterested",
        "linkedinNotInterested",
        "linkedinSendFailed",
        "manualInterested",
