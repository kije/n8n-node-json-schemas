{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Netlify Trigger",
  "description": "Handle netlify events via webhooks",
  "x-n8n-node-type": "netlifyTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "siteId": {
      "x-title": "Site Name or ID",
      "description": "Select the Site ID. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "event": {
      "x-title": "Event",
      "default": "",
      "x-required": true,
      "type": "string",
      "enum": [
        "deployBuilding",
        "deployFailed",
        "deployCreated",
        "submissionCreated"
      ],
      "x-enumNames": [
        "Deploy Building",
        "Deploy Failed",
        "Deploy Created",
        "Form Submitted"
      ]
    },
    "formId": {
      "x-title": "Form Name or ID",
      "description": "Select a form. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": "",
      "x-displayOptions": {
        "show": {
          "event": [
            "submissionCreated"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "simple": {
      "x-title": "Simplify",
      "description": "Whether to return a simplified version of the response instead of the raw data",
      "default": true,
      "x-displayOptions": {
        "show": {
          "event": [
            "submissionCreated"
          ]
        }
      },
      "type": "boolean"
    }
  },
  "x-subtitle": "={{$parameter[\"event\"]}}",
  "x-icon": "file:netlify.svg",
  "x-inputs": [],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "netlifyApi",
      "required": true
    }
  ],
  "x-webhooks": "true"
}
