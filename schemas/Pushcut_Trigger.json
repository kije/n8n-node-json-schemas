{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Pushcut Trigger",
  "description": "Starts the workflow when Pushcut events occur",
  "x-n8n-node-type": "pushcutTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "actionName": {
      "x-title": "Action Name",
      "description": "Choose any name you would like. It will show up as a server action in the app.",
      "default": "",
      "type": "string"
    }
  },
  "x-icon": "file:pushcut.png",
  "x-inputs": [],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "pushcutApi",
      "required": true
    }
  ],
  "x-webhooks": "true"
}
