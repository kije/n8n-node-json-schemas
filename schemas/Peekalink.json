{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Peekalink",
  "description": "Consume the Peekalink API",
  "x-n8n-node-type": "peekalink",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "output"
  ],
  "type": "object",
  "properties": {
    "operation": {
      "x-title": "Operation",
      "default": "preview",
      "type": "string",
      "enum": [
        "isAvailable",
        "preview"
      ],
      "x-enumNames": [
        "Is Available",
        "Preview"
      ],
      "x-enumDescriptions": [
        "Check whether preview for a given link is available",
        "Return the preview for a link"
      ]
    },
    "url": {
      "x-title": "URL",
      "default": "",
      "x-required": true,
      "type": "string"
    }
  },
  "x-subtitle": "={{$parameter[\"operation\"]",
  "x-icon": "file:peekalink.png",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "peekalinkApi",
      "required": true
    }
  ]
}
