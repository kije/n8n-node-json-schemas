{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GetResponse Trigger",
  "description": "Starts the workflow when GetResponse events occur",
  "x-n8n-node-type": "getResponseTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "authentication": {
      "x-title": "Authentication",
      "default": "apiKey",
      "type": "string",
      "enum": [
        "apiKey",
        "oAuth2"
      ],
      "x-enumNames": [
        "API Key",
        "OAuth2"
      ]
    },
    "events": {
      "x-title": "Events",
      "default": [],
      "x-required": true,
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "subscribe",
          "unsubscribe",
          "click",
          "open",
          "survey"
        ]
      },
      "x-enumNames": [
        "Customer Subscribed",
        "Customer Unsubscribed",
        "Email Clicked",
        "Email Opened",
        "Survey Submitted"
      ]
    },
    "listIds": {
      "x-title": "List Names or IDs",
      "description": "Choose from the list, or specify IDs using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>",
      "default": [],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "options": {
      "x-title": "Options",
      "default": {},
      "type": "object",
      "properties": {
        "delete": {
          "x-title": "Delete Current Subscription",
          "description": "Whether to delete the current subscription",
          "default": false,
          "type": "boolean"
        }
      }
    }
  },
  "x-icon": "file:getResponse.png",
  "x-inputs": [],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "getResponseApi",
      "required": true,
      "displayOptions": {
        "show": {
          "authentication": [
            "apiKey"
          ]
        }
      }
    },
    {
      "name": "getResponseOAuth2Api",
      "required": true,
      "displayOptions": {
        "show": {
          "authentication": [
            "oAuth2"
          ]
        }
      }
    }
  ],
  "x-webhooks": "true"
}
