{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Asana Trigger",
  "description": "Starts the workflow when Asana events occur.",
  "x-n8n-node-type": "asanaTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "authentication": {
      "x-title": "Authentication",
      "default": "accessToken",
      "type": "string",
      "enum": [
        "accessToken",
        "oAuth2"
      ],
      "x-enumNames": [
        "Access Token",
        "OAuth2"
      ]
    },
    "resource": {
      "x-title": "Resource",
      "description": "The resource ID to subscribe to. The resource can be a task or project.",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "workspace": {
      "x-title": "Workspace Name or ID",
      "description": "The workspace ID the resource is registered under. This is only required if you want to allow overriding existing webhooks. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": "",
      "type": "string",
      "enum": [],
      "x-enumNames": []
    }
  },
  "x-icon": "file:asana.svg",
  "x-inputs": [],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "asanaApi",
      "required": true,
      "displayOptions": {
        "show": {
          "authentication": [
            "accessToken"
          ]
        }
      }
    },
    {
      "name": "asanaOAuth2Api",
      "required": true,
      "displayOptions": {
        "show": {
          "authentication": [
            "oAuth2"
          ]
        }
      }
    }
  ],
  "x-webhooks": "true"
}
