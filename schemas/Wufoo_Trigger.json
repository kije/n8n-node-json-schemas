{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Wufoo Trigger",
  "description": "Handle Wufoo events via webhooks",
  "x-n8n-node-type": "wufooTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "form": {
      "x-title": "Forms Name or ID",
      "description": "The form upon which will trigger this node when a new entry is made. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "onlyAnswers": {
      "x-title": "Only Answers",
      "description": "Whether to return only the answers of the form and not any of the other data",
      "default": true,
      "type": "boolean"
    }
  },
  "x-icon": "file:wufoo.png",
  "x-inputs": [],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "wufooApi",
      "required": true
    }
  ],
  "x-webhooks": "true"
}
