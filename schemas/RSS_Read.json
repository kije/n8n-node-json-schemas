{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RSS Read",
  "description": "Reads data from an RSS Feed",
  "x-n8n-node-type": "rssFeedRead",
  "x-n8n-version": [
    1,
    1.1,
    1.2
  ],
  "x-n8n-group": [
    "input"
  ],
  "type": "object",
  "properties": {
    "url": {
      "x-title": "URL",
      "description": "URL of the RSS feed",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "options": {
      "x-title": "Options",
      "default": {},
      "type": "object",
      "properties": {
        "customFields": {
          "x-title": "Custom Fields",
          "description": "A comma-separated list of custom fields to include in the output. For example, \"author, contentSnippet\".",
          "default": "",
          "type": "string"
        },
        "ignoreSSL": {
          "x-title": "Ignore SSL Issues (Insecure)",
          "description": "Whether to ignore SSL/TLS certificate issues or not",
          "default": false,
          "type": "boolean"
        }
      }
    }
  },
  "x-icon": "fa:rss",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ]
}
