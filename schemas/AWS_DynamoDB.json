{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AWS DynamoDB",
  "description": "Consume the AWS DynamoDB API",
  "x-n8n-node-type": "awsDynamoDb",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "transform"
  ],
  "type": "object",
  "properties": {
    "authentication": {
      "x-title": "Authentication",
      "default": "iam",
      "type": "string",
      "enum": [
        "iam",
        "assumeRole"
      ],
      "x-enumNames": [
        "AWS (IAM)",
        "AWS (Assume Role)"
      ]
    },
    "resource": {
      "x-title": "Resource",
      "default": "item",
      "type": "string",
      "enum": [
        "item"
      ],
      "x-enumNames": [
        "Item"
      ]
    },
    "operation": {
      "x-title": "Operation",
      "default": "upsert",
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ]
        }
      },
      "type": "string",
      "enum": [
        "upsert",
        "delete",
        "get",
        "getAll"
      ],
      "x-enumNames": [
        "Create or Update",
        "Delete",
        "Get",
        "Get Many"
      ],
      "x-enumDescriptions": [
        "Create a new record, or update the current one if it already exists (upsert)",
        "Delete an item",
        "Get an item",
        "Get many items"
      ]
    },
    "tableName": {
      "x-title": "Table Name or ID",
      "description": "Table to operate on. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": [],
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ]
        }
      },
      "x-required": true,
      "type": "string"
