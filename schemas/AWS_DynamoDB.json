        "show": {
          "operation": [
            "upsert"
          ],
          "dataToSend": [
            "defineBelow"
          ]
        }
      },
      "type": "object",
      "properties": {
        "fieldValues": {
          "x-title": "Field",
          "type": "array",
          "items": {
            "type": "object",
