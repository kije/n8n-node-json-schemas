          "x-title": "Expression Attribute Names",
          "description": "One or more substitution tokens for attribute names in an expression. <a href=\"https://docs.aws.amazon.com/amazondynamodb/latest/APIReference/API_PutItem.html\">View details</a>.",
          "default": {},
          "type": "object",
          "properties": {
            "eanValues": {
              "x-title": "Expression",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "key": {
                    "x-title": "Key",
                    "default": "",
                    "type": "string"
                  },
                  "value": {
                    "x-title": "Value",
                    "default": "",
                    "type": "string"
                  }
                }
              }
            }
          }
