      },
      "type": "boolean"
    },
    "select": {
      "x-title": "Select",
      "default": "ALL_ATTRIBUTES",
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ],
          "operation": [
            "getAll"
          ]
        }
      },
      "type": "string",
      "enum": [
        "ALL_ATTRIBUTES",
        "ALL_PROJECTED_ATTRIBUTES",
        "COUNT",
        "SPECIFIC_ATTRIBUTES"
      ],
      "x-enumNames": [
        "All Attributes",
        "All Projected Attributes",
        "Count",
        "Specific Attributes"
      ],
      "x-enumDescriptions": [
        "",
        "",
        "",
        "Select them in Attributes to Select under Additional Fields"
      ]
