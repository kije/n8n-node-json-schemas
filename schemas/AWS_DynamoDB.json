              },
              "type": {
                "x-title": "Type",
                "default": "S",
                "type": "string",
                "enum": [
                  "B",
                  "N",
                  "S"
                ],
                "x-enumNames": [
                  "Binary",
                  "Number",
                  "String"
                ]
              },
              "value": {
                "x-title": "Value",
                "default": "",
                "type": "string"
              }
            }
          }
        }
      }
