      "x-displayOptions": {
        "show": {
          "operation": [
            "upsert"
          ]
        }
      },
      "type": "string",
      "enum": [
        "autoMapInputData",
        "defineBelow"
      ],
      "x-enumNames": [
        "Auto-Map Input Data to Columns",
        "Define Below for Each Column"
      ],
      "x-enumDescriptions": [
        "Use when node input properties match destination column names",
        "Set the value for each destination column"
      ]
