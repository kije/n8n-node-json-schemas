{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Webflow",
  "description": "Consume the Webflow API",
  "x-n8n-node-type": "webflow",
  "x-n8n-version": 2,
  "x-n8n-group": [
    "transform"
  ],
  "type": "object",
  "properties": {
    "resource": {
      "x-title": "Resource",
      "default": "item",
      "type": "string",
      "enum": [
        "item"
      ],
      "x-enumNames": [
        "Item"
      ]
    },
    "operation": {
      "x-title": "Operation",
      "default": "get",
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ]
        }
      },
      "type": "string",
      "enum": [
        "create",
        "deleteItem",
        "get",
        "getAll",
        "update"
      ],
      "x-enumNames": [
        "Create",
        "Delete",
        "Get",
        "Get Many",
        "Update"
      ]
    },
    "siteId": {
      "x-title": "Site Name or ID",
      "description": "ID of the site containing the collection whose items to add to. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": "",
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ],
          "operation": [
            "update"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "collectionId": {
      "x-title": "Collection Name or ID",
      "description": "ID of the collection to add an item to. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": "",
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ],
          "operation": [
            "update"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "live": {
      "x-title": "Live",
      "description": "Whether the item should be published on the live site",
      "default": false,
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ],
          "operation": [
            "update"
          ]
        }
      },
      "x-required": true,
      "type": "boolean"
    },
    "fieldsUi": {
      "x-title": "Fields",
      "default": {},
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ],
          "operation": [
            "update"
          ]
        }
      },
      "type": "object",
      "properties": {
        "fieldValues": {
          "x-title": "Field",
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "fieldId": {
                "x-title": "Field Name or ID",
                "description": "Field to set for the item to create. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
                "default": "",
                "type": "string"
              },
              "fieldValue": {
                "x-title": "Field Value",
                "description": "Value to set for the item to create",
                "default": "",
                "type": "string"
              }
            }
          }
        }
      }
    },
    "itemId": {
      "x-title": "Item ID",
      "description": "ID of the item to update",
      "default": "",
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ],
          "operation": [
            "update"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "returnAll": {
      "x-title": "Return All",
      "description": "Whether to return all results or only up to a given limit",
      "default": false,
      "x-displayOptions": {
        "show": {
          "resource": [
            "item"
          ],
          "operation": [
            "getAll"
          ]
        }
      },
      "type": "boolean"
    },
    "limit": {
      "x-title": "Limit",
      "description": "Max number of results to return",
      "default": 100,
      "x-displayOptions": {
        "show": {
          "returnAll": [
            false
          ],
          "resource": [
            "item"
          ],
          "operation": [
            "getAll"
          ]
        }
      },
      "type": "number",
      "minimum": 1,
      "maximum": 100
    }
  },
  "x-subtitle": "={{$parameter[\"operation\"] + \": \" + $parameter[\"resource\"]}}",
  "x-icon": "file:webflow.svg",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "webflowOAuth2Api",
      "required": true
    }
  ]
}
