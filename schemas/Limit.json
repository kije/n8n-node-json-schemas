{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Limit",
  "description": "Restrict the number of items",
  "x-n8n-node-type": "limit",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "transform"
  ],
  "type": "object",
  "properties": {
    "maxItems": {
      "x-title": "Max Items",
      "description": "If there are more items than this number, some are removed",
      "default": 1,
      "type": "number",
      "minimum": 1
    },
    "keep": {
      "x-title": "Keep",
      "description": "When removing items, whether to keep the ones at the start or the ending",
      "default": "firstItems",
      "type": "string",
      "enum": [
        "firstItems",
        "lastItems"
      ],
      "x-enumNames": [
        "First Items",
        "Last Items"
      ]
    }
  },
  "x-icon": "file:limit.svg",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ]
}
