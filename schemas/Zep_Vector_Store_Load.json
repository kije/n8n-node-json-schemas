{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Zep Vector Store: Load",
  "description": "Load data from Zep Vector Store index",
  "x-n8n-node-type": "vectorStoreZepLoad",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "transform"
  ],
  "type": "object",
  "properties": {
    "collectionName": {
      "x-title": "Collection Name",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "options": {
      "x-title": "Options",
      "default": {},
      "type": "object",
      "properties": {
        "embeddingDimensions": {
          "x-title": "Embedding Dimensions",
          "description": "Whether to allow using characters from the Unicode surrogate blocks",
          "default": 1536,
          "type": "number"
        },
        "metadata": {
          "x-title": "Metadata Filter",
          "description": "Metadata to filter the document by",
          "default": {},
          "type": "object",
          "properties": {
            "metadataValues": {
              "x-title": "Fields to Set",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "x-title": "Name",
                    "default": "",
                    "x-required": true,
                    "type": "string"
                  },
                  "value": {
                    "x-title": "Value",
                    "default": "",
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "x-icon": "file:zep.png",
  "x-inputs": [
    {
      "displayName": "Embedding",
      "maxConnections": 1,
      "type": "ai_embedding",
      "required": true
    }
  ],
  "x-outputs": [
    "ai_vectorStore"
  ],
  "x-credentials": [
    {
      "name": "zepApi",
      "required": true
    }
  ]
}
