{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PayPal Trigger",
  "description": "Handle PayPal events via webhooks",
  "x-n8n-node-type": "payPalTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "events": {
      "x-title": "Event Names or IDs",
      "description": "The event to listen to. Choose from the list, or specify IDs using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": [],
      "x-required": true,
      "type": "array",
      "items": {
        "type": "string",
        "enum": []
      },
      "x-enumNames": []
    }
  },
  "x-icon": "file:paypal.svg",
  "x-inputs": [],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "payPalApi",
      "required": true
    }
  ],
  "x-webhooks": "true"
}
