{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Embeddings Google Vertex",
  "description": "Use Google Vertex Embeddings",
  "x-n8n-node-type": "embeddingsGoogleVertex",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "transform"
  ],
  "type": "object",
  "properties": {
    "projectId": {
      "x-title": "Project ID",
      "description": "Select or enter your Google Cloud project ID",
      "default": {
        "mode": "list",
        "value": ""
      },
      "x-required": true,
      "type": "object",
      "properties": {
        "__rl": {
          "type": "boolean",
          "const": true
        },
        "mode": {
          "type": "string",
          "enum": [
            "list",
            "id"
          ]
        },
        "value": {
          "type": "string"
        }
      },
      "required": [
        "mode",
        "value"
      ],
      "x-modes": [
        {
          "name": "list",
          "displayName": "From List",
          "type": "list"
        },
        {
          "name": "id",
          "displayName": "ID",
          "type": "string"
        }
      ]
    },
    "modelName": {
      "x-title": "Model Name",
      "description": "The model which will generate the embeddings. <a href=\"https://cloud.google.com/vertex-ai/generative-ai/docs/model-reference/text-embeddings-api\">Learn more</a>.",
      "default": "text-embedding-005",
      "type": "string"
    }
  },
  "x-icon": "file:google.svg",
  "x-inputs": [],
  "x-outputs": [
    "ai_embedding"
  ],
  "x-credentials": [
    {
      "name": "googleApi",
      "required": true
    }
  ]
}
