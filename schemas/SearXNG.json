{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SearXNG",
  "description": "Search in SearXNG",
  "x-n8n-node-type": "toolSearXng",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "transform"
  ],
  "type": "object",
  "properties": {
    "options": {
      "x-title": "Options",
      "default": {},
      "type": "object",
      "properties": {
        "numResults": {
          "x-title": "Number of Results",
          "default": 10,
          "type": "number"
        },
        "pageNumber": {
          "x-title": "Search Page Number",
          "default": 1,
          "type": "number"
        },
        "language": {
          "x-title": "Language",
          "description": "Defines the language to use. It's a two-letter language code. (e.g., `en` for English, `es` for Spanish, or `fr` for French). Head to <a href=\"https://docs.searxng.org/user/search-syntax.html#select-language\">SearXNG search syntax page</a> for more info.",
          "default": "en",
          "type": "string"
        },
        "safesearch": {
          "x-title": "Safe Search",
          "description": "Filter search results of engines which support safe search",
          "default": 0,
          "type": "string",
          "enum": [
            0,
            1,
            2
          ],
          "x-enumNames": [
            "None",
            "Moderate",
            "Strict"
          ]
        }
      }
    }
  },
  "x-icon": "file:searXng.svg",
  "x-inputs": [],
  "x-outputs": [
    "ai_tool"
  ],
  "x-credentials": [
    {
      "name": "searXngApi",
      "required": true
    }
  ]
}
