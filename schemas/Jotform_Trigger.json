{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Jotform Trigger",
  "description": "Handle Jotform events via webhooks",
  "x-n8n-node-type": "jotFormTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "form": {
      "x-title": "Form Name or ID",
      "description": "Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "resolveData": {
      "x-title": "Resolve Data",
      "description": "By default does the webhook-data use internal keys instead of the names. If this option gets activated, it will resolve the keys automatically to the actual names.",
      "default": true,
      "type": "boolean"
    },
    "onlyAnswers": {
      "x-title": "Only Answers",
      "description": "Whether to return only the answers of the form and not any of the other data",
      "default": true,
      "type": "boolean"
    }
  },
  "x-icon": {
    "light": "file:jotform.svg",
    "dark": "file:jotform.dark.svg"
  },
  "x-inputs": [],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "jotFormApi",
      "required": true
    }
  ],
  "x-webhooks": "true"
}
