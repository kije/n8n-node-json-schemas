{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SurveyMonkey Trigger",
  "description": "Starts the workflow when Survey Monkey events occur",
  "x-n8n-node-type": "surveyMonkeyTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "authentication": {
      "x-title": "Authentication",
      "default": "accessToken",
      "type": "string",
      "enum": [
        "accessToken",
        "oAuth2"
      ],
      "x-enumNames": [
        "Access Token",
        "OAuth2"
      ]
    },
    "objectType": {
      "x-title": "Type",
      "default": "",
      "x-required": true,
      "type": "string",
      "enum": [
        "collector",
        "survey"
      ],
      "x-enumNames": [
        "Collector",
        "Survey"
      ]
    },
    "event": {
      "x-title": "Event",
      "default": "",
      "x-displayOptions": {
        "show": {
          "objectType": [
            "collector"
          ]
        }
      },
      "x-required": true,
      "type": "string",
      "enum": [
        "collector_deleted",
        "collector_updated",
        "response_completed",
        "response_created",
        "response_deleted",
        "response_disqualified",
        "response_overquota",
        "response_updated"
      ],
      "x-enumNames": [
        "Collector Deleted",
        "Collector Updated",
        "Response Completed",
        "Response Created",
        "Response Deleted",
        "Response Disqualified",
        "Response Overquota",
        "Response Updated"
      ],
      "x-enumDescriptions": [
        "A collector is deleted",
        "A collector is updated",
        "A survey response is completed",
        "A respondent begins a survey",
        "A response is deleted",
        "A survey response is disqualified",
        "A response is over a surveyâ€™s quota",
        "A survey response is updated"
      ]
    },
    "surveyIds": {
      "x-title": "Survey Names or IDs",
      "description": "Choose from the list, or specify IDs using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>",
      "default": [],
      "x-displayOptions": {
        "show": {
          "objectType": [
            "survey"
          ]
        },
        "hide": {
          "event": [
            "survey_created"
          ]
        }
      },
      "x-required": true,
      "type": "array",
      "items": {
        "type": "string",
        "enum": []
      },
      "x-enumNames": []
    },
    "surveyId": {
      "x-title": "Survey Name or ID",
      "description": "Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>",
      "default": [],
      "x-displayOptions": {
        "show": {
          "objectType": [
            "collector"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "collectorIds": {
      "x-title": "Collector Names or IDs",
      "description": "Choose from the list, or specify IDs using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>",
      "default": [],
      "x-displayOptions": {
        "show": {
          "objectType": [
            "collector"
          ]
        }
      },
      "x-required": true,
      "type": "array",
      "items": {
        "type": "string",
        "enum": []
      },
      "x-enumNames": []
    },
    "resolveData": {
      "x-title": "Resolve Data",
      "description": "By default the webhook-data only contain the IDs. If this option gets activated, it will resolve the data automatically.",
      "default": true,
      "x-displayOptions": {
        "show": {
          "event": [
            "response_completed"
          ]
        }
      },
      "type": "boolean"
    },
    "onlyAnswers": {
      "x-title": "Only Answers",
      "description": "Whether to return only the answers of the form and not any of the other data",
      "default": true,
      "x-displayOptions": {
        "show": {
          "resolveData": [
            true
          ],
          "event": [
            "response_completed"
          ]
        }
      },
      "type": "boolean"
    }
  },
  "x-icon": "file:surveyMonkey.svg",
  "x-inputs": [],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "surveyMonkeyApi",
      "required": true,
      "displayOptions": {
        "show": {
          "authentication": [
            "accessToken"
          ]
        }
      }
    },
    {
      "name": "surveyMonkeyOAuth2Api",
      "required": true,
      "displayOptions": {
        "show": {
          "authentication": [
            "oAuth2"
          ]
        }
      }
    }
  ],
  "x-webhooks": "true"
}
