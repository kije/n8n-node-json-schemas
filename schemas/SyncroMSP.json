{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SyncroMSP",
  "description": "Gets data from SyncroMSP",
  "x-n8n-node-type": "syncroMsp",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "output"
  ],
  "type": "object",
  "properties": {
    "resource": {
      "x-title": "Resource",
      "default": "contact",
      "type": "string",
      "enum": [
        "contact",
        "customer",
        "rmm",
        "ticket"
      ],
      "x-enumNames": [
        "Contact",
        "Customer",
        "RMM",
        "Ticket"
      ]
    },
    "operation": {
      "x-title": "Operation",
      "default": "getAll",
      "x-displayOptions": {
        "show": {
          "resource": [
            "rmm"
          ]
        }
      },
      "type": "string",
      "enum": [
        "create",
        "delete",
        "get",
        "getAll",
        "mute"
      ],
      "x-enumNames": [
        "Create",
        "Delete",
        "Get",
        "Get Many",
        "Mute"
      ],
      "x-enumDescriptions": [
        "Create new RMM Alert",
        "Delete RMM Alert",
        "Retrieve RMM Alert",
        "Retrieve many RMM Alerts",
        "Mute RMM Alert"
      ]
    },
    "returnAll": {
      "x-title": "Return All",
      "description": "Whether to return all results or only up to a given limit",
      "default": false,
      "x-displayOptions": {
        "show": {
          "resource": [
            "rmm"
          ],
          "operation": [
            "getAll"
          ]
        }
      },
      "type": "boolean"
    },
    "limit": {
      "x-title": "Limit",
      "description": "Max number of results to return",
      "default": 25,
      "x-displayOptions": {
        "show": {
          "resource": [
            "rmm"
          ],
          "operation": [
            "getAll"
          ],
          "returnAll": [
            false
          ]
        }
      },
      "type": "number",
      "minimum": 1
    },
    "filters": {
      "x-title": "Filters",
      "default": {},
      "x-displayOptions": {
        "show": {
          "resource": [
            "rmm"
          ],
          "operation": [
            "getAll"
          ]
        }
      },
      "type": "object",
      "properties": {
        "status": {
          "x-title": "Status",
          "default": "all",
          "type": "string",
          "enum": [
            "active",
            "all",
            "resolved"
          ],
          "x-enumNames": [
            "Active",
            "All",
            "Resolved"
          ]
        }
      }
