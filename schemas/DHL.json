{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DHL",
  "description": "Consume DHL API",
  "x-n8n-node-type": "dhl",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "input"
  ],
  "type": "object",
  "properties": {
    "resource": {
      "x-title": "Resource",
      "default": "shipment",
      "type": "string"
    },
    "operation": {
      "x-title": "Operation",
      "default": "get",
      "x-displayOptions": {
        "show": {
          "resource": [
            "shipment"
          ]
        }
      },
      "type": "string",
      "enum": [
        "get"
      ],
      "x-enumNames": [
        "Get Tracking Details"
      ]
    },
    "trackingNumber": {
      "x-title": "Tracking Number",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "options": {
      "x-title": "Options",
      "default": {},
      "type": "object",
      "properties": {
        "recipientPostalCode": {
          "x-title": "Recipient's Postal Code",
          "description": "DHL will return more detailed information on the shipment when you provide the Recipient's Postal Code - it acts as a verification step",
          "default": "",
          "type": "string"
        }
      }
    }
  },
  "x-subtitle": "={{$parameter[\"operation\"] + \": \" + $parameter[\"resource\"]}}",
  "x-icon": "file:dhl.svg",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "dhlApi",
      "required": true,
      "testedBy": "dhlApiCredentialTest"
    }
  ]
}
