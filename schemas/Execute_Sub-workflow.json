{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Execute Sub-workflow",
  "description": "Execute another workflow",
  "x-n8n-node-type": "executeWorkflow",
  "x-n8n-version": [
    1,
    1.1,
    1.2,
    1.3
  ],
  "x-n8n-group": [
    "transform"
  ],
  "type": "object",
  "properties": {
    "operation": {
      "x-title": "Operation",
      "default": "call_workflow",
      "type": "string"
    },
    "source": {
      "x-title": "Source",
      "description": "Where to get the workflow to execute from",
      "default": "database",
      "x-displayOptions": {
        "show": {
          "@version": [
            {
              "_cnd": {
                "gte": 1.2
              }
            }
          ]
        }
      },
      "type": "string",
      "enum": [
        "database",
        "parameter"
      ],
      "x-enumNames": [
        "Database",
        "Define Below"
      ],
      "x-enumDescriptions": [
        "Load the workflow from the database by ID",
        "Pass the JSON code of a workflow"
      ]
    },
    "workflowId": {
      "x-title": "Workflow",
      "default": "",
      "x-displayOptions": {
        "show": {
          "source": [
            "database"
          ],
          "@version": [
            {
              "_cnd": {
                "gte": 1.1
              }
            }
          ]
        }
      },
      "x-required": true,
      "type": "string",
      "x-n8n-type": "workflowSelector"
    },
    "workflowPath": {
      "x-title": "Workflow Path",
      "description": "The path to local JSON workflow file to execute",
      "default": "",
      "x-displayOptions": {
        "show": {
          "source": [
            "localFile"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "workflowJson": {
      "x-title": "Workflow JSON",
      "description": "The workflow JSON code to execute",
      "default": "\n\n\n",
      "x-displayOptions": {
        "show": {
          "source": [
            "parameter"
          ]
        }
      },
      "x-required": true,
      "oneOf": [
        {
          "type": "string",
          "description": "JSON string"
        },
        {
          "type": "object"
        },
        {
          "type": "array"
        }
      ]
    },
    "workflowUrl": {
      "x-title": "Workflow URL",
      "description": "The URL from which to load the workflow from",
      "default": "",
      "x-displayOptions": {
        "show": {
          "source": [
            "url"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "workflowInputs": {
      "x-title": "Workflow Inputs",
      "default": {
        "mappingMode": "defineBelow",
        "value": null
      },
      "x-displayOptions": {
        "show": {
          "source": [
            "database"
          ],
          "@version": [
            {
              "_cnd": {
                "gte": 1.2
              }
            }
          ]
        },
        "hide": {
          "workflowId": [
            ""
          ]
        }
      },
      "x-required": true,
      "type": "string",
      "x-n8n-type": "resourceMapper"
    },
    "mode": {
      "x-title": "Mode",
      "default": "once",
      "type": "string",
      "enum": [
        "once",
        "each"
      ],
      "x-enumNames": [
        "Run once with all items",
        "Run once for each item"
      ],
      "x-enumDescriptions": [
        "Pass all items into a single execution of the sub-workflow",
        "Call the sub-workflow individually for each item"
      ]
    },
    "options": {
      "x-title": "Options",
      "default": {},
      "type": "object",
      "properties": {
        "waitForSubWorkflow": {
          "x-title": "Wait For Sub-Workflow Completion",
          "description": "Whether the main workflow should wait for the sub-workflow to complete its execution before proceeding",
          "default": true,
          "type": "boolean"
        }
      }
    }
  },
  "x-subtitle": "={{\"Workflow: \" + $parameter[\"workflowId\"]}}",
  "x-icon": "fa:sign-in-alt",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ]
}
