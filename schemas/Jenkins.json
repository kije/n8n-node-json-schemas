{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Jenkins",
  "description": "Consume Jenkins API",
  "x-n8n-node-type": "jenkins",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "output"
  ],
  "type": "object",
  "properties": {
    "resource": {
      "x-title": "Resource",
      "default": "job",
      "type": "string",
      "enum": [
        "build",
        "instance",
        "job"
      ],
      "x-enumNames": [
        "Build",
        "Instance",
        "Job"
      ]
    },
    "operation": {
      "x-title": "Operation",
      "default": "getAll",
      "x-displayOptions": {
        "show": {
          "resource": [
            "build"
          ]
        }
      },
      "type": "string",
      "enum": [
        "getAll"
      ],
      "x-enumNames": [
        "Get Many"
      ],
      "x-enumDescriptions": [
        "List Builds"
      ]
    },
    "job": {
      "x-title": "Job Name or ID",
      "description": "Name of the job. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": "",
      "x-displayOptions": {
        "show": {
          "resource": [
            "build"
          ],
          "operation": [
            "getAll"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "param": {
      "x-title": "Parameters",
      "description": "Parameters for Jenkins job",
      "default": {},
      "x-displayOptions": {
        "show": {
          "resource": [
            "job"
          ],
          "operation": [
            "triggerParams"
          ]
        }
      },
      "x-required": true,
      "type": "object",
      "properties": {
        "params": {
          "x-title": "Parameters",
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "x-title": "Name or ID",
                "description": "Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>",
                "default": "",
                "type": "string"
              },
              "value": {
                "x-title": "Value",
                "default": "",
                "type": "string"
              }
            }
          }
        }
      }
    },
    "newJob": {
      "x-title": "New Job Name",
      "description": "Name of the new Jenkins job",
      "default": "",
      "x-displayOptions": {
        "show": {
          "resource": [
            "job"
          ],
          "operation": [
            "copy",
            "create"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "xml": {
      "x-title": "XML",
      "description": "XML of Jenkins config",
      "default": "",
      "x-displayOptions": {
        "show": {
          "resource": [
            "job"
          ],
          "operation": [
            "create"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "reason": {
      "x-title": "Reason",
      "description": "Freeform reason for quiet down mode",
      "default": "",
      "x-displayOptions": {
        "show": {
          "resource": [
            "instance"
          ],
          "operation": [
            "quietDown"
          ]
        }
      },
      "type": "string"
    },
    "returnAll": {
      "x-title": "Return All",
      "description": "Whether to return all results or only up to a given limit",
      "default": false,
      "x-displayOptions": {
        "show": {
          "resource": [
            "build"
          ],
          "operation": [
            "getAll"
          ]
        }
      },
      "type": "boolean"
    },
    "limit": {
      "x-title": "Limit",
      "description": "Max number of results to return",
      "default": 50,
      "x-displayOptions": {
        "show": {
          "resource": [
            "build"
          ],
          "operation": [
            "getAll"
          ],
          "returnAll": [
            false
          ]
        }
      },
      "type": "number",
      "minimum": 1
    }
  },
  "x-subtitle": "={{$parameter[\"operation\"] + \": \" + $parameter[\"resource\"]}}",
  "x-icon": "file:jenkins.svg",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "jenkinsApi",
      "required": true,
      "testedBy": "jenkinApiCredentialTest"
    }
  ]
}
