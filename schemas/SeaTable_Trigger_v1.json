{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SeaTable Trigger",
  "description": "Starts the workflow when SeaTable events occur",
  "x-n8n-node-type": "seaTableTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "trigger"
  ],
  "type": "object",
  "properties": {
    "tableName": {
      "x-title": "Table Name or ID",
      "description": "The name of SeaTable table to access. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "event": {
      "x-title": "Event",
      "default": "rowCreated",
      "type": "string",
      "enum": [
        "rowCreated"
      ],
      "x-enumNames": [
        "Row Created"
      ],
      "x-enumDescriptions": [
        "Trigger on newly created rows"
      ]
    },
    "simple": {
      "x-title": "Simplify",
      "description": "Whether to return a simplified version of the response instead of the raw data",
      "default": true,
      "type": "boolean"
    }
  },
  "x-subtitle": "={{$parameter[\"event\"]}}",
  "x-icon": "file:seaTable.svg",
  "x-inputs": [],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "seaTableApi",
      "required": true
    }
  ]
}
