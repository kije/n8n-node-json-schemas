{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AWS Textract",
  "description": "Sends data to Amazon Textract",
  "x-n8n-node-type": "awsTextract",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "output"
  ],
  "type": "object",
  "properties": {
    "authentication": {
      "x-title": "Authentication",
      "default": "iam",
      "type": "string",
      "enum": [
        "iam",
        "assumeRole"
      ],
      "x-enumNames": [
        "AWS (IAM)",
        "AWS (Assume Role)"
      ]
    },
    "operation": {
      "x-title": "Operation",
      "default": "analyzeExpense",
      "type": "string",
      "enum": [
        "analyzeExpense"
      ],
      "x-enumNames": [
        "Analyze Receipt or Invoice"
      ]
    },
    "binaryPropertyName": {
      "x-title": "Input Data Field Name",
      "description": "The name of the input field containing the binary file data to be uploaded. Supported file types: PNG, JPEG.",
      "default": "data",
      "x-displayOptions": {
        "show": {
          "operation": [
            "analyzeExpense"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "simple": {
      "x-title": "Simplify",
      "description": "Whether to return a simplified version of the response instead of the raw data",
      "default": true,
      "x-displayOptions": {
        "show": {
          "operation": [
            "analyzeExpense"
          ]
        }
      },
      "type": "boolean"
    }
  },
  "x-subtitle": "={{$parameter[\"operation\"]}}",
  "x-icon": "file:textract.svg",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "aws",
      "required": true,
      "displayOptions": {
        "show": {
          "authentication": [
            "iam"
          ]
        }
      }
    },
    {
      "name": "awsAssumeRole",
      "required": true,
      "displayOptions": {
        "show": {
          "authentication": [
            "assumeRole"
          ]
        }
      }
    }
  ]
}
