{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Read Binary File",
  "description": "Reads a binary file from disk",
  "x-n8n-node-type": "readBinaryFile",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "input"
  ],
  "type": "object",
  "properties": {
    "filePath": {
      "x-title": "File Path",
      "description": "Path of the file to read",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "dataPropertyName": {
      "x-title": "Property Name",
      "description": "Name of the binary property to which to write the data of the read file",
      "default": "data",
      "x-required": true,
      "type": "string"
    }
  },
  "x-icon": "fa:file-import",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ]
}
