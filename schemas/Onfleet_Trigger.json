        "taskArrival",
        "taskAssigned",
        "taskCloned",
        "taskCompleted",
        "taskCreated",
        "taskDelayed",
        "taskDeleted",
        "taskEta",
        "taskFailed",
        "taskStarted",
        "taskUnassigned",
        "taskUpdated",
        "workerCreated",
        "workerDeleted",
        "workerDuty"
