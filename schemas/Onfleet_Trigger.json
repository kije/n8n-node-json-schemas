{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Onfleet Trigger",
  "description": "Starts the workflow when Onfleet events occur",
  "x-n8n-node-type": "onfleetTrigger",
  "x-n8n-version": 1,
  "x-n8n-group": [
