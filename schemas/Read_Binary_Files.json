{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Read Binary Files",
  "description": "Reads binary files from disk",
  "x-n8n-node-type": "readBinaryFiles",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "input"
  ],
  "type": "object",
  "properties": {
    "fileSelector": {
      "x-title": "File Selector",
      "description": "Pattern for files to read",
      "default": "",
      "x-required": true,
      "type": "string"
    },
    "dataPropertyName": {
      "x-title": "Property Name",
      "description": "Name of the binary property to which to write the data of the read files",
      "default": "data",
      "x-required": true,
      "type": "string"
    }
  },
  "x-icon": "fa:file-import",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ]
}
