{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Mindee",
  "description": "Consume Mindee API",
  "x-n8n-node-type": "mindee",
  "x-n8n-version": [
    1,
    2,
    3
  ],
  "x-n8n-group": [
    "input"
  ],
  "type": "object",
  "properties": {
    "apiVersion": {
      "x-title": "API Version",
      "description": "Which Mindee API Version to use",
      "default": 4,
      "x-displayOptions": {
        "show": {
          "@version": [
            3
          ]
        }
      },
      "type": "string",
      "enum": [
        1,
        3,
        4
      ],
      "x-enumNames": [
        "1",
        "3",
        "4"
      ]
    },
    "resource": {
      "x-title": "Resource",
      "default": "receipt",
      "type": "string",
      "enum": [
        "invoice",
        "receipt"
      ],
      "x-enumNames": [
        "Invoice",
        "Receipt"
      ]
    },
    "operation": {
      "x-title": "Operation",
      "default": "predict",
      "type": "string",
      "enum": [
        "predict"
      ],
      "x-enumNames": [
        "Predict"
      ]
    },
    "binaryPropertyName": {
      "x-title": "Input Binary Field",
      "default": "data",
      "x-displayOptions": {
        "show": {
          "operation": [
            "predict"
          ],
          "resource": [
            "receipt",
            "invoice"
          ]
        }
      },
      "x-required": true,
      "type": "string"
    },
    "rawData": {
      "x-title": "RAW Data",
      "description": "Whether to return the data exactly in the way it got received from the API",
      "default": false,
      "type": "boolean"
    }
  },
  "x-subtitle": "={{$parameter[\"operation\"] + \": \" + $parameter[\"resource\"]}}",
  "x-icon": "file:mindee.svg",
  "x-inputs": [
    "main"
  ],
  "x-outputs": [
    "main"
  ],
  "x-credentials": [
    {
      "name": "mindeeReceiptApi",
      "required": true,
      "displayOptions": {
        "show": {
          "resource": [
            "receipt"
          ]
        }
      }
    },
    {
      "name": "mindeeInvoiceApi",
      "required": true,
      "displayOptions": {
        "show": {
          "resource": [
            "invoice"
          ]
        }
      }
    }
  ]
}
