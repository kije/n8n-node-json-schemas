{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Embeddings Azure OpenAI",
  "description": "Use Embeddings Azure OpenAI",
  "x-n8n-node-type": "embeddingsAzureOpenAi",
  "x-n8n-version": 1,
  "x-n8n-group": [
    "transform"
  ],
  "type": "object",
  "properties": {
    "model": {
      "x-title": "Model (Deployment) Name",
      "description": "The name of the model(deployment) to use",
      "default": "",
      "type": "string"
    },
    "options": {
      "x-title": "Options",
      "description": "Additional options to add",
      "default": {},
      "type": "object",
      "properties": {
        "batchSize": {
          "x-title": "Batch Size",
          "description": "Maximum number of documents to send in each request",
          "default": 512,
          "type": "number",
          "maximum": 2048
        },
        "stripNewLines": {
          "x-title": "Strip New Lines",
          "description": "Whether to strip new lines from the input text",
          "default": true,
          "type": "boolean"
        },
        "timeout": {
          "x-title": "Timeout",
          "description": "Maximum amount of time a request is allowed to take in seconds. Set to -1 for no timeout.",
          "default": -1,
          "type": "number"
        },
        "dimensions": {
          "x-title": "Dimensions",
          "description": "The number of dimensions the resulting output embeddings should have. Only supported in text-embedding-3 and later models.",
          "default": 1536,
          "type": "string",
          "enum": [
            256,
            512,
            1024,
            1536,
            3072
          ],
          "x-enumNames": [
            "256",
            "512",
            "1024",
            "1536",
            "3072"
          ]
        }
      }
    }
  },
  "x-icon": "file:azure.svg",
  "x-inputs": [],
  "x-outputs": [
    "ai_embedding"
  ],
  "x-credentials": [
    {
      "name": "azureOpenAiApi",
      "required": true
    }
  ]
}
